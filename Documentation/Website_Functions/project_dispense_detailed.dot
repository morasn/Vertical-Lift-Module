    SelectProducts [label=<<B>Select products for project</B>>, shape=parallelogram, style=filled, fillcolor="#fff9e5"];
    Dispense [label=<<B>Click 'Project Dispense'</B>>, shape=rectangle, style=filled, fillcolor="#fff9e5"];
    Backend [label=<<B>Backend logs transaction</B>>, shape=rectangle, style=filled, fillcolor="#ffe5b4"];
